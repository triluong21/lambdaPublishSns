{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Live Lambda",
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}/node_modules/serverless/bin/serverless",
      "stopOnEntry": false,
      "env": {
        "AWS_PROFILE": "personal",
        "AWS_REGION": "us-east-2",
        "Region": "us-east-2",
        "AccountId": "160116285639",
      },      
      "args": [
        "invoke",
        "local",
        "-f",
        "publishSns",
      ],
      "cwd": "${workspaceRoot}",
      "protocol": "inspector",
      "outFiles": [
        "${workspaceFolder}/.webpack/**/*.js"
      ]
    },
    {
      "name": "Debug ts mocha",
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}/node_modules/mocha/bin/_mocha",
      "stopOnEntry": false,
      "env": {
        "AWS_PROFILE": "personal",
        "AWS_REGION": "us-east-2",
        "Region": "us-east-2",
        "AccountId": "160116285639",
      },
      "args": [
        "--timeout",
        "999999",
        "--require",
        "ts-node/register",
        "--recursive",
        "${workspaceRoot}/test/*.spec.ts"
        // "--require",
        // "mocha.env"
      ],
      "cwd": "${workspaceRoot}",
      "protocol": "inspector",
      "outFiles": [
        "${workspaceFolder}/.webpack/**/*.js"
      ]
    },
  ]
}